{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react", "unused-imports"
    ],
    "rules": {
        "indent": ["warn", 2],
        "react/prop-types": 0,
        "linebreak-style": ["warn", "windows"],
        "quotes": ["warn", "single"],
        "semi": ["warn", "always"],
        "no-unused-vars":"off",
        "no-undef":"off",
        "unused-imports/no-unused-imports": "error"
    }
}
